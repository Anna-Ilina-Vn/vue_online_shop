 },\n      clearSearchField() {\n        this.searchValue = ''\n        this.GET_SEARCH_VALUE_TO_VUEX();\n        if (this.$route.path !== '/catalog') {\n          this.$router.push('/catalog')\n        }\n      }\n    }\n  }\n</script>\n\n<style lang=\"scss\">\n\n  footer {\n    width: 100%;\n\tposition: relative;\n  bottom: 0;\n\n  .modal-footer {\n  flex: 1 1 auto;\n\tbackground: sandybrown;\n  height: 100%;\n\tcolor: white;\n\twidth: 250px;\n\tborder-radius: 4px;\n\ttext-align: center;\n\tpadding: 10px 20px 10px 20px;\n\tposition: relative;\n\t\n\t.modal__icon {\n\t\tposition: absolute;\n    width: 150px;\n    height: 100px;\n    top: 20px;\n    left: 170px;\n    margin-left: -185px;\n    background: transparent;\n    border-radius: 50%;\n    z-index: 2;\n\t}\n\n   input {\n      height: 34px;\n      line-height: 34px;\n      font-size: 20px;\n      font-family: 'Cormorant Garamond', serif;\n      padding: 0 10px;\n      background: #ffffff;\n      border: 1px solid #DDDDDD;\n      color: #808080;\n      outline: none;\n      border-radius: 5px;\n    }\n\n    p {\n      line-height: 34px;\n      font-size: 20px;\n      font-family: 'Cormorant Garamond', serif;\n      text-align: center;\n      font-weight: bolder;\n      text-transform: uppercase;\n      z-index: 5;\n    }\n    .email-box {\n     position: relative;\n      div {\n        padding: 0 5px;\n        position: absolute;\n        right: -20px;\n        top: 54px;\n      }\n    }\n    .material-icons {\n      i {\n        font-style: normal;\n      }\n    }\n}\n.contacts {\n  flex: 1 1 auto;\n  position: relative;\n  padding: 0 30px;\n  text-align: left;\n\n  div{\n    position: inherit;\n  }\n\n  a {\n    line-height: 34px;\n    font-size: 20px;\n    font-family: 'Cormorant Garamond', serif;\n    text-align: center;\n    color: #2c3e50;\n  }\n  i {\n    position: absolute;\n    top: 4px;\n    left: -30px;\n  }\n}\n  .v-footer {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    width: 100%;\n    padding: 5px 30px;\n    height: 150px;\n    border-top: 4px solid lightseagreen;\n\n    .nav-footer {\n      line-height: 24px;\n      flex: 1 1 20%;\n      font-size: 16px;\n      font-family: 'Cormorant Garamond', serif;\n      display: flex;\n      flex-direction: column;\n      font-weight: bolder;\n\n      a {\n        color: #2c3e50;\n        text-transform: uppercase;\n      }\n    }\n\n    .img {\n      position: relative;\n      flex: 1 1 20%;\n      height: 150px;\n      z-index: -10;\n\n      img {\n        height: 100%;\n      }\n    }\n  }\n      }\n</style>\n"]}]}